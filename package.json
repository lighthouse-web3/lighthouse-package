    "@peculiar/webcrypto": "^1.4.0",
    "bls-eth-wasm": "^1.0.4",
    "cli-spinner": "^0.2.10",
    "commander": "^10.0.0",
    "conf": "^10.1.1",
    "crypto-js": "^4.2.0",
    "ethers": "^6.7.1",
    "fs-extra": "^11.1.1",
    "kleur": "^4.1.5",
    "mime-types": "^2.1.35",
    "progress": "^2.0.3",
    "read": "^1.0.7",
    "recursive-fs": "^2.1.0"
